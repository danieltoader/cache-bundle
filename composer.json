{
  "name": "emag-tech-labs/cache-bundle",
  "description": "CacheBundle by eMAGTechLabs",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "eMAGTechLabs",
      "email": "techlabs@emag.ro",
      "homepage": "https://techlabs.emag.ro",
      "role": "Maintainer"
    },
    {
      "name": "Daniel Toader",
      "email": "developer@danieltoader.com",
      "role": "Developer"
    },
    {
      "name": "Alexandru Bumbacea",
      "email": "alexandru.bumbacea@emag.ro",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "ocramius/proxy-manager": "2.1.*",
    "psr/log": "1.*",
    "psr/cache": "1.0.*",
    "doctrine/annotations": "~1.3",
    "psr/container": "^1.0",
    "symfony/dependency-injection": ">3.3"
  },
  "suggest": {
    "symfony/cache": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "3.*",
    "phpmd/phpmd": "@stable",
    "phing/phing": "@stable",
    "sebastian/phpcpd": "~2",
    "symfony/cache": "3.*",
    "satooshi/php-coveralls": "~1.0",
    "symfony/monolog-bundle": "@stable",
    "symfony/framework-bundle": "@stable",
    "symfony/http-kernel": "3.*",
    "symfony/phpunit-bridge": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "EmagTechLabs\\CacheBundle\\": "src/"
    },
    "psr-0": {
      "EmagTechLabs\\CacheBundle\\Annotation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EmagTechLabs\\CacheBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
